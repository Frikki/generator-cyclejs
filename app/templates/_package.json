{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "My <%= superb %> module",
  "license": "MIT",
  "bugs": "https://github.com/<%= githubUserName %>/<%= moduleName %>/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubUserName %>/<%= moduleName %>"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= emailAddress %>",
    "url": "<%= humanizedWebsiteUrl %>"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "lint": "eslint ./**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "postcss -c postcss.config.json",
    "browserify": "browserify demo/main.js -d -t [ babelify --optional runtime ] --outfile demo/bundle.js",
    "build": "npm run lint && npm run css && npm run browserify",
    "init": "npm install && validate-commit-msg",
    "start": "npm run init && npm run build && echo 'Open demo/index.html in your browser'",
    "patch": "release patch",
    "minor": "release minor",
    "major": "release major"
  },
  "files": [
    "<%= moduleName %>.js"
  ],
  "keywords": [],
  "dependencies": {
    "template-demo-pages": "git://github.com/CyclicMaterials/template-demo-pages"
  },
  "devDependencies": {
    "postcss": "^4.0.0",
    "postcss-cli": "^1.0.0"
  }
}
